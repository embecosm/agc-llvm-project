#ifndef ELF_RELOC
#error "ELF_RELOC must be defined"
#endif

// No relocation
ELF_RELOC(R_AGC_NONE,        0)
// General 16-bit relocation
ELF_RELOC(R_AGC_16,          1)
// 12-bit constant pool address relocation
ELF_RELOC(R_AGC_CPI12,       2)
// 12-bit address of bank select config relocation
ELF_RELOC(R_AGC_BANKS12,     3)
// low 12 bits of address relocation
ELF_RELOC(R_AGC_LO12,       4)
